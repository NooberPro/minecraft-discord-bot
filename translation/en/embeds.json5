{
  // Translation for commands embeds.
  ip: {
    // Variables: {ip}
    title: '__**SERVER ADDRESS**__',
    description: '**{ip}**',
  },
  site: {
    // Variables: {site}
    title: '__**WEBSITE**__',
    description: '**Link: {site}**',
  },
  version: {
    // Variables: {version}
    title: '__**VERSION**__',
    description: '**{version}**',
  },
  offlineEmbed: {
    // Variables : {ip},{version}, {site}
    title: ':red_circle: OFFLINE',
    description: '', // Leave description blank to disable it.
  },
  players: {
    // Variables: {playeronline}, {playermax}
    title: '__**PLAYERS**__',
    description: '**{playeronline}/{playermax}**',
    // After description will be the Player Name List.
    // Shows offline Embed when MC server is offline.
  },
  motd: {
    // Variables: {motd}
    title: '__**MOTD**__',
    description: '**{motd}**',
  },
  onlineEmbed: {
    // Variables: {ip}, {motd}, {version}, {site}
    title: ':green_circle: ONLINE',
    // Here, the Player List Embed title and description from players embed above (line: 26)

    // After the player List Field will be the description.
    description_field_one: '__**MOTD**__\n**{motd}**\n\n__**SERVER ADDRESS**__\n**{ip}**',
    description_field_two: '__**VERSION**__\n**{version}**\n\n__**WEBSITE**__\n**Link: {site}**',
  },
  help: {
    //Variables: {prefix}, {botName}
    title: '__**COMMANDS LIST**__',
    description: '**Prefix: `{prefix}`**',
    // Variables for listFormat Only: {cmdDescription}, {cmdSlashMention}
    listFormat: '{slashCmdMention} **`-` {cmdDescription}**',
  },
}
