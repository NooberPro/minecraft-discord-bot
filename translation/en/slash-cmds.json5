    name: 'help',
    description: 'Sends the list of available commands.',
  },
  ip: {
    name: 'ip',
    description: 'Sends the Server Address of the Minecraft Server.',
  },
  site: {
    name: 'site',
    description: "Sends the Minecraft Server's website/vote link.",
  },
  version: {
    name: 'version',
    description: 'Sends the version of the Minecraft Server.',
  },

