{
  // Traduction pour les messages intégrés de commandes.
  ip: {
    // Variables : {ip}
    title: '__**ADRESSE DU SERVEUR**__',
    description: '**{ip}**',
  },
  site: {
    // Variables : {site}
    title: '__**SITE WEB**__',
    description: '**Lien : {site}**',
  },
  version: {
    // Variables : {version}
    title: '__**VERSION**__',
    description: '**{version}**',
  },
  offlineEmbed: {
    // Variables : {ip}, {version}, {site}
    title: ':red_circle: HORS LIGNE',
    description: '', // Pour désactiver cela, laissez la description vide.
    footer: 'Vérifié le',
  },
  players: {
    // Variables : {playeronline}, {playermax}
    title: '__**JOUEURS**__',
    description: '**{playeronline}/{playermax}**',
    // La liste des noms de joueurs sera affichée après la description.
    // Affiche des intégrations hors ligne lorsque le serveur MC est hors ligne.
  },
  motd: {
    // Variables : {motd}
    title: '__**MOTD**__',
    description: '**{motd}**',
  },
  onlineEmbed: {
    // Variables : {ip}, {motd}, {version}, {site}
    title: ':green_circle: EN LIGNE',
    // Ici, le titre et la description de la liste des joueurs de ci-dessus (ligne : 28)

    // La description est placée après le champ de la liste des joueurs.
    description_field_one: '__**MOTD**__\n**{motd}**\n\n__**ADRESSE DU SERVEUR**__\n**{ip}**',
    description_field_two: '__**VERSION**__\n**{version}**\n\n__**SITE WEB**__\n**Lien : {site}**',
    footer: 'Vérifié le',
  },
  help: {
    // Variables : {prefix}, {botName}
    title: '__**LISTE DES COMMANDES**__',
    description: '**Préfixe : `{prefix}`**',
    // Seules les variables de format de liste : {cmdDescription}, {cmdSlashMention}.
    listFormat: '{slashCmdMention} **`-` {cmdDescription}**',
  },
  info: {
    title: '**Informations et statistiques du bot**',
    // Variables : {cpuUsage}, {memoryUsage}, {nodeVersion}, {uptimeDays}, {uptimeHours}, {uptimeMinutes}, {uptimeSeconds}, {ping}, {websocket}
    description: "**Utilisation du CPU : `{cpuUsage}%`\nUtilisation de la mémoire : `{memoryUsage}MB`\nVersion de Node.js : `{nodeVersion}`\nDurée d'activité du bot : `{uptimeDays}` jours, `{uptimeHours}` heures, `{uptimeMinutes}` minutes, `{uptimeSeconds}` secondes\nPing : Client `{ping}ms` | Websocket : `{websocket}ms`**",
  },
}
