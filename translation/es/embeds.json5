{
  ip: {
    // Variables: {ip}
    title: '__**DIRECCIÓN DEL SERVIDOR**__',
    description: '**{ip}**',
  },
  site: {
    // Variables: {site}
    title: '__**SITIO WEB**__',
    description: '**Enlace: {site}**',
  },
  version: {
    // Variables: {version}
    title: '__**VERSIÓN**__',
    description: '**{version}**',
  },
  offlineEmbed: {
    // Variables : {ip}, {version}, {site}
    title: ':red_circle: DESCONEXIONADO',
    description: '', // Deje en blanco la descripción para deshabilitarla.
  },
  players: {
    // Variables: {playeronline}, {playermax}
    title: '__**JUGADORES**__',
    description: '**{playeronline}/{playermax}**',
    // Después de la descripción estará la lista de nombres de jugadores.
    // Muestra el mensaje de desconexión cuando el servidor de Minecraft está desconectado.
  },
  motd: {
    // Variables: {motd}
    title: '__**MOTD**__',
    description: '**{motd}**',
  },
  onlineEmbed: {
    // Variables: {ip}, {motd}, {version}, {site}
    title: ':green_circle: EN LÍNEA',
    // Aquí, el título y la descripción del Embed de la lista de jugadores desde el embed de jugadores arriba (línea: 26)

    // Después del campo de la lista de jugadores estará la descripción.
    description_field_one: '__**MOTD**__\n**{motd}**\n\n__**DIRECCIÓN DEL SERVIDOR**__\n**{ip}**',
    description_field_two: '__**VERSIÓN**__\n**{version}**\n\n__**SITIO WEB**__\n**Enlace: {site}**',
  },
  help: {
    //Variables: {prefix}, {botName}
    title: '__**LISTA DE COMANDOS**__',
    description: '**Prefijo: `{prefix}`**',
    // Variables para el formato de lista solamente: {cmdDescription}, {cmdSlashMention}
    listFormat: '{slashCmdMention} **`-` {cmdDescription}**',
  },
}
